{
  "name": "otaku.online.server",
  "version": "0.0.1",
  "description": "api server for the long delayed cosplay market.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "db-migrate up all",
    "migrate:reset": "db-migrate reset && yarn run migrate",
    "db:reset": "yarn run migrate:reset && yarn run seeders",
    "seeders": "babel-node seeders/run.js --presets,stage-2",
    "start": "nodemon app/index.js --exec babel-node --presets,stage-2 --plugins babel-root-import",
    "build": "babel app -d build --presets es2015,stage-2",
    "start:prod": "node build/index.js"
  },
  "dependencies": {
    "body-parser": "1.15.2",
    "compression": "1.6.2",
    "connect-pg-simple": "3.1.2",
    "cors": "2.8.1",
    "db-migrate": "0.10.0-beta.20",
    "db-migrate-pg": "0.1.11",
    "dotenv": "4.0.0",
    "express": "4.14.0",
    "fbgraph": "1.4.1",
    "jwt-simple": "0.5.1",
    "lodash": "4.17.4",
    "moment": "2.17.1",
    "multer": "1.2.1",
    "passport": "0.3.2",
    "passport-jwt": "2.2.1",
    "pg": "6.1.2",
    "pg-promise": "5.5.1",
    "unirest": "0.5.1",
    "uuid": "3.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-root-import": "^4.1.5"
  },
  "author": "Aldrich Allen Barcenas",
  "license": "ISC"
}
